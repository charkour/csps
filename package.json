{
  "homepage": "https://github.com/charkour/csps",
  "bugs": {
    "url": "https://github.com/charkour/csps/issues"
  },
  "description": "Tools to solve constraint satisfaction problems",
  "version": "1.0.5",
  "license": "MIT",
  "keywords": [
    "constraint-satisfaction-problems",
    "aima",
    "typescript",
    "min-conflicts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/charkour/csps.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint src test",
    "fix": "tsdx lint src test --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "build:docs": "typedoc --out docs src --theme node_modules/eledoc/bin/default/"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint src test"
    }
  },
  "name": "csps",
  "author": "Charles Kornoelje",
  "module": "dist/csps.esm.js",
  "size-limit": [
    {
      "path": "dist/csps.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/csps.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/jest": "26.0.19",
    "@types/seedrandom": "2.4.28",
    "eledoc": "0.2.1",
    "husky": "^4.3.6",
    "seedrandom": "3.0.5",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typedoc": "0.20.14",
    "typescript": "^4.1.3"
  }
}
