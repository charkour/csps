{
  "name": "csps",
  "version": "2.0.2",
  "description": "🛠️ Tools to solve constraint satisfaction problems",
  "keywords": [
    "constraint-satisfaction-problems",
    "aima",
    "typescript",
    "min-conflicts"
  ],
  "homepage": "https://github.com/charkour/csps",
  "bugs": {
    "url": "https://github.com/charkour/csps/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/charkour/csps.git"
  },
  "license": "MIT",
  "author": "Charles Kornoelje",
  "main": "dist/index.js",
  "module": "dist/csps.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "build:docs": "typedoc --out docs src --theme node_modules/eledoc/bin/default/",
    "fix": "tsdx lint src test --fix",
    "lint": "tsdx lint src test",
    "prepare": "tsdx build && husky install",
    "size": "size-limit",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "4.11.0",
    "@types/jest": "26.0.23",
    "@types/seedrandom": "3.0.1",
    "eledoc": "0.2.1",
    "husky": "6.0.0",
    "seedrandom": "3.0.5",
    "size-limit": "4.11.0",
    "tsdx": "0.14.1",
    "tslib": "2.2.0",
    "typedoc": "0.20.36",
    "typescript": "4.3.2"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=10"
  },
  "size-limit": [
    {
      "path": "dist/csps.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/csps.esm.js",
      "limit": "10 KB"
    }
  ]
}
